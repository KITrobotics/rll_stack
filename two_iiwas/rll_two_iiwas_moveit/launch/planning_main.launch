<?xml version="1.0"?>
<launch>
    <arg name="robot_name_1" default="iiwa_1"/>
    <arg name="robot_name_2" default="iiwa_2"/>

    <!-- main moveit launch file -->
    <include file="$(find rll_two_iiwas_moveit)/launch/moveit_planning_execution.launch">
        <arg name="robot_name_1" value="$(arg robot_name_1)" />
        <arg name="robot_name_2" value="$(arg robot_name_2)" />
    </include>

    <!-- gripper setup -->
    <node ns="$(arg robot_name_1)" name="gripper_egl_90" pkg="schunk_gripper_egl90" type="schunk_gripper_egl90_node" output="screen">
        <param name="can_iface" value="can0"/>
    </node>
    <node ns="$(arg robot_name_2)" name="gripper_egl_90" pkg="schunk_gripper_egl90" type="schunk_gripper_egl90_node" output="screen">
        <param name="can_iface" value="can1"/>
    </node>

</launch>
